            h2 Exame de #{exame.nome.primeiro + " " + exame.nome.último}
        br
        h3 Data: #{exame.dataEMD}
        h3 Nome: #{exame.nome.primeiro + " " + exame.nome.último}
        h3 Idade: #{exame.idade}
        h3 Género: #{exame.género}
        h3 Morada: #{exame.morada}
        h3 Modalidade: #{exame.modalidade}
        h3 Clube: #{exame.clube}
